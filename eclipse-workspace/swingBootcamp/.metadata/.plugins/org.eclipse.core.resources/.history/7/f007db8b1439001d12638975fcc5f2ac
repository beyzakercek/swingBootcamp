package vowels;

public class Main {

	public static void main(String[] args) {
		char [] vowels = {'a', 'e', 'i', 'o' ,'u'};
		String name = "beyza";
		for(int i = 0; i < name.length(); i++) {
			for(int j = 0; j < vowels.length; j++) {
				if(name.charAt(i) == vowels[i]) {
					System.out.println(name.charAt(i));
				}

			}
		}
	}

}
